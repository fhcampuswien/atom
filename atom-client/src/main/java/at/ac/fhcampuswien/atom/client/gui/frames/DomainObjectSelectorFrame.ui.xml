<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gui="urn:import:at.ac.fhcampuswien.atom.client.gui"
	xmlns:f="urn:import:at.ac.fhcampuswien.atom.client.gui.frames">
	<ui:style field="panelStyle" type='at.ac.fhcampuswien.atom.client.gui.frames.DomainObjectSelectorFrame.PanelStyle'>
		.space {
			width: 100%;
			height: 100%;
		}
		
		.fullWidth {
			width: 100%;
		}
	</ui:style>

	<g:DockLayoutPanel styleName='{panelStyle.space}' unit='PX'>
		<g:west size="20">
			<g:HTML styleName='{panelStyle.space}' />
		</g:west>
		<g:east size="10">
			<g:HTML styleName='{panelStyle.space}' />
		</g:east>
		<g:north size="10">
			<g:HTML styleName='{panelStyle.space}' />
		</g:north>
		<g:center>
			<g:SimplePanel ui:field="root" styleName='{panelStyle.space}' />
		</g:center>
	</g:DockLayoutPanel>


</ui:UiBinder> 
