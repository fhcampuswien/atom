				width: 100%;
			}
			
			.textBox {
				height: 26px;
				padding-left: 3px;				
			}
		</ui:style>

	<g:TextBox ui:field="textBox" styleName='{res.css.attributeViewGeneral} {panelStyle.textBox}' />

