<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:dnd="urn:import:at.ac.fhcampuswien.atom.client.gui.dnd">

	<ui:with field='res'
		type='at.ac.fhcampuswien.atom.client.gui.AtomClientBundle' />

	<ui:style field="panelStyle"
		type='at.ac.fhcampuswien.atom.client.gui.attributes.SuggestBoxView.PanelStyle'>
			.space {
				height: 26px;
				width: 100%;
			}
			
			.suggestBox {
				color: #575A5F;
				
				border-top: 1px solid #CCC;
				border-right: 1px solid #999;
				border-bottom: 1px solid #999;
				border-left: 1px solid #999;
				
				-moz-box-sizing:border-box;
    			-webkit-box-sizing:border-box;
				box-sizing: border-box; 
				
				font-family: Verdana, Arial, Helvetica, sans-serif;
				font-size: 11px;
				/*border: 2px #C9D5E0 inset; /*solid*/
				height: 26px;
				width: 100%;				
				
				padding: 3px 3px 3px 0px;
				
				 /*box-sizing: content-box;
			    -moz-box-sizing:content-box;
			    -webkit-box-sizing:content-box;*/
			}
		</ui:style>

	<g:SuggestBox ui:field="suggestBox" styleName='{panelStyle.suggestBox}' />

